{
  "name": "21st-dev-scraper",
  "version": "1.0.0",
  "type": "module",
  "description": "Smart scraper for 21st.dev components with batch and resume capabilities",
  "scripts": {
    "scrape": "tsx scraper.ts",
    "scrape:top50": "tsx scraper.ts --limit=50",
    "scrape:top200": "tsx scraper.ts --limit=200 --batch=50",
    "scrape:all": "tsx scraper.ts --limit=1000 --batch=100",
    "resume": "tsx scraper.ts --resume",
    "test": "tsx scraper.ts --limit=5"
  },
  "dependencies": {
    "playwright": "^1.49.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
